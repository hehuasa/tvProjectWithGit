{
  "name": "ant-design-pro",
  "version": "1.3.0",
  "description": "An out-of-box UI solution for enterprise applications",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "cross-env ESLINT=none BROWSER=none port=8005 roadhog dev",
    "start:no-proxy": "cross-env NO_PROXY=true DISABLE_ESLINT=true roadhog dev",
    "build": "cross-env ESLINT=none --max_old_space_size=4096 roadhog build",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "cross-env ANALYZE=true roadhog build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "roadhog test",
    "test:component": "roadhog test ./src/components",
    "test:all": "node ./tests/run-tests.js"
  },
  "dependencies": {
    "@antv/data-set": "^0.8.0",
    "@babel/polyfill": "^7.0.0",
    "antd": "^3.9.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.9.2",
    "bizcharts": "^3.4.2",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "classnames": "^2.2.5",
    "crypto-js": "^3.1.9-1",
    "dva": "^2.1.0",
    "dva-loading": "^2.0.1",
    "enquire-js": "^0.2.1",
    "esri-loader": "^2.5.0",
    "esri-loader-react": "^2.0.3",
    "express-http-proxy": "^1.4.0",
    "fastclick": "^1.0.6",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "lodash-decorators": "^4.4.1",
    "moment": "^2.19.1",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.2",
    "rc-drawer-menu": "^0.5.0",
    "rc-tree": "^1.14.6",
    "rc-tree-select": "^1.12.9",
    "react": "^16.5.2",
    "react-beautiful-dnd": "^9.0.2",
    "react-color": "^2.17.0",
    "react-container-query": "^0.9.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.5.2",
    "react-draggable": "^3.0.5",
    "react-fittext": "^1.0.0",
    "react-quill": "^1.2.7",
    "react-slick": "^0.20.0",
    "react-zmage": "^0.6.5",
    "slick-carousel": "^1.8.1",
    "url-polyfill": "^1.1.0",
    "wangeditor": "^3.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.9.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.6.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.8",
    "eslint-plugin-react": "^7.0.1",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "lint-staged": "^6.0.0",
    "mockjs": "^1.0.1-beta3",
    "pro-download": "^1.0.1",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.11.1",
    "roadhog": "^2.4.9",
    "roadhog-api-doc": "^0.3.4",
    "rollbar": "^2.4.7",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "optionalDependencies": {
    "nightmare": "^2.10.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
